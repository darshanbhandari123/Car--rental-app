<%= form_with(model: login, local: true) do |form| %>
    <div class="field">
      <%= form.label :email_id %>
      <%= form.text_field :email_id, id: :user_email_id %>
    </div>

    <div class="field">
      <%= form.label :password %>
      <%= form.password_field :password, id: :user_password %>
    </div>

    <div class="actions">
      <%= form.submit %>
    </div>
<% end %>