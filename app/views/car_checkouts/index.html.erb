<p id="notice"><%= notice %></p>

<h1>Car Checkouts</h1>

<table>
  <thead>
    <tr>
      <th>License</th>
      <th>Checkout by</th>
      <th>Duration</th>
      <th>Checkout at</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @car_checkouts.each do |car_checkout| %>
      <tr>
        <td><%= car_checkout.license %></td>
        <td><%= car_checkout.checkout_by %></td>
        <td><%= car_checkout.duration %></td>
        <td><%= car_checkout.checkout_at %></td>
        <td><%= link_to 'Show', car_checkout %></td>
        <td><%= link_to 'Edit', edit_car_checkout_path(car_checkout) %></td>
        <td><%= link_to 'Destroy', car_checkout, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Car Checkout', new_car_checkout_path %>
