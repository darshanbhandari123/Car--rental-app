<%= form_with(local: true) do |form| %>

    <div class="field">
      <%= form.label :manufacturer %>
      <%= form.text_field :manufacturer, id: :suggestion_manufacturer %>
    </div>


    <div class="field">
      <%= form.label :model %>
      <%= form.text_field :model, id: :car_model %>
    </div>


    <div class="actions">
      <%= form.submit %>
    </div>
<% end %>
