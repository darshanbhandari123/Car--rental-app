<div>
  <h1>
    Welcome <%= @user['name'] %>
  </h1>

  <span class="logout"><%= link_to 'Logout', logout_path %></span>

  <div class="div-search">

  </div>

  <% if @user['u_type'] == 1 %>
      <%= render 'superadmin.html.erb', cars: @cars, user: @user%>
  <% end %>

  <% if @user['u_type'] == 2 %>
      <%= render 'admin.html.erb', cars: @cars, user: @user%>
  <% end %>

  <% if @user['u_type'] == 3 %>
    <%= render 'customer.html.erb', cars: @cars, user: @user%>
  <% end %>

</div>


<h2>Checkedout history</h2>
<%= render 'checkoutHistory.html.erb', user: @user%>